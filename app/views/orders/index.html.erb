<% @orders.each do |order| %>
  <%= link_to order.id, profile_order_path(order) if current_registered? %>
  <%= link_to order.id, admin_order_path(order) if current_admin? %>
  <%= order.created_at.strftime("%B, %d %Y at %I:%M %p") %>
  <%= order.updated_at.strftime("%B, %d %Y at %I:%M %p") %>
  <%= order.quantity_of_items %>
  <%= order.grand_total %>
  <%= order.status %>
<% end %>
