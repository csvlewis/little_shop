<% if @user && @user.id %>
  <% case @user.role %>
    <% when 'registered' %>
      <%= render partial: 'nav/registered' %>
    <% when 'merchant'%>
      <%= render partial: 'nav/merchant' %>
    <% when 'admin' %>
      <%= render partial: 'nav/admin' %>
  <% end %>
<% else %>
  <%= render partial: "nav/visitor" %>
<% end %>
